<h1>Reservations</h1>

<table class="table">
  <thead>
    <tr>
      <th>From</th>
      <th>To</th>
      <th>Rental charge</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reservations.each do |reservation| %>
      <tr>
        <td><%= reservation.from %></td>
        <td><%= reservation.to %></td>
        <td><%= reservation.rental_charge %></td>
        <td><%= link_to 'Show', admin_reservation_path(reservation) %></td>
        <td><%= link_to 'Edit', edit_admin_reservation_path(reservation) %></td>
        <td><%= link_to 'Destroy', admin_reservation_path(reservation), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>